{
  "name": "open-video-share",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "copy:simple-peer": "cpy node_modules/simple-peer/simplepeer.min.js public --flat",
    "dev": "vite --open",
    "generate:manifest-icons": "pwa-asset-generator public/open-video-share-icon.png public/icons --favicon -i ./index.html",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prebuild": "npm run copy:simple-peer",
    "predev": "npm run copy:simple-peer",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {}\"",
    "preview": "vite preview",
    "test": "playwright test -c playwright.config.ts",
    "tsc": "tsc"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@mantine/core": "^6.0.11",
    "@mantine/hooks": "^6.0.11",
    "@mantine/notifications": "^6.0.19",
    "@tabler/icons-react": "^2.30.0",
    "buffer": "^6.0.3",
    "husky": "8.0.3",
    "i18next": "^23.4.4",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.1",
    "pako": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.1.1",
    "react-qr-code": "^2.0.12",
    "react-zxing": "^1.1.4",
    "simple-peer": "^9.11.1",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.34.3",
    "@types/node": "^20.4.9",
    "@types/pako": "^2.0.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/simple-peer": "^9.11.5",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.0.4",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.46.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "prettier": "^3.0.1",
    "prettier-plugin-pkg": "0.18.0",
    "prettier-plugin-sh": "^0.13.1",
    "pwa-asset-generator": "^6.3.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-pwa": "^0.16.4"
  }
}
